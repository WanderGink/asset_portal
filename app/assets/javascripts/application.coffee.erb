ready = ->
  $("img").error ->
    console.log 'imgError!!!!'

    img = $(this)

    img.attr('src', <%= asset_path("missing.png").to_json %>)
    img.css('padding', '40px')
    img.insertBefore("")
    img.unbind('error')

$(document).ready ready
$(document).on 'page:load', ready